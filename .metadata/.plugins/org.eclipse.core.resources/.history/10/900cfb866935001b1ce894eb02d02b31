package br.com.universidadexyz.implementacao;

import javax.swing.JOptionPane;

import br.com.universidadexyz.beans.Aluno;

public class TestAluno {

	public static void main(String[] args) {
		Aluno objAluno = new Aluno();
		objAluno.preencherCpf("12345678909");
		objAluno.preencherNome("Joãozinho");
		objAluno.preencherRm(123456);
		
		JOptionPane.showMessageDialog(null, "Nome: " + objAluno.retornarNome() + 
				"\nRM: " + objAluno.retornarRm() + 
				"\nCPF: " + objAluno.retornarCpf());
		

	}

}
                            