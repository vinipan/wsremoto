public class TesteExcecao {

	public static void main(String[] args) {
		try {
			int numero = Integer.parseInt("A");
			System.out.println("numero: " + numero);
		}
		catch (Exception erro ) 
			{System.out.println("ERRO!");}
		
		finally {}
		
		
		
		
		
		
		
		

	}

}
